<%  for project in projects -%>

<% if project.node_templates.any? -%>

<% if project != @project %>
<tr>
  <%= content_tag('th', project_path(project), :colspan => 6, :align => 'left') %>
</tr>
<% end -%>

<% for template in project.node_templates.sort -%>
<%= render :partial => 'template_line', :locals => {:template=>template} %>
<% end -%>

<% end -%>

<% end -%>

