<%= klass_menu("Editing #{@klass.name}") %>

<p><%= "Needed attributes for this class:" %></p>

<% labelled_tabular_form_for :custom_klass, @klass, :url => ({ :action=>'configure', :id=>@klass }) do |f| %>

  <%= render :partial => "form", :locals => {:f=>f} %>

  <%=submit_tag 'Save'%>

<% end %>

