<%=klass_menu "Choose a class to copy"%>

<% labelled_tabular_form_for :copy_klass, @klass, :url=>{:action=>'create',:id=>@node} do |f| -%>

  <%= render :partial => 'tabular_form', :locals=>{:f=>f} %>

  <p><%= submit_tag("Save") %></p>

<% end -%>

<p><%= link_to "Back", :controller => 'klass', :action => 'list', :id => @node %></p>
