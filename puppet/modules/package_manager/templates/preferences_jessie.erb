##################
# Puppet Managed #
##################
#
# Highest priority to stable
Package: *
Pin: release n=jessie
Pin-Priority: 700

<% pfw = scope.lookupvar('::packages_from_stretch')
   pfw = [] unless pfw.is_a?(Array)
   pfw.uniq.each do |package|
-%>
# we want <%=package%> from stretch
Package: <%=package%>
Pin: release n=stretch
Pin-Priority: 700

<% end -%>
